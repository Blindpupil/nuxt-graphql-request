{
  "name": "nuxt-graphql-request",
  "version": "2.0.0",
  "description": "Easy GraphQL client integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxtjs",
    "nuxt.js",
    "graphql",
    "graphql-request",
    "nuxt-graphql",
    "nuxt-graphql-request"
  ],
  "typings": "types/index.d.ts",
  "main": "lib/module.js",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "repository": "https://github.com/Gomah/nuxt-graphql-request",
  "homepage": "https://github.com/Gomah/nuxt-graphql-request",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test:system": "NODE_ENV=test jest --testRegex \"/test/system/(.+)\\.test\\.ts$\"",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "watch": "tsc --watch",
    "test": "yarn test:lint && NODE_ENV=test jest",
    "test:lint": "eslint --ext .js, lib/module.js"
  },
  "peerDependencies": {
    "graphql": "14.x || 15.x"
  },
  "dependencies": {
    "graphql-request": "^3.0.0",
    "isomorphic-unfetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@nuxt/types": "^2.14.1",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^14.0.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "nuxt": "^2.14.1",
    "prettier": "^2.0.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "standard-version": "^8.0.2"
  }
}
