{
  "name": "nuxt-graphql-request",
  "version": "1.0.0",
  "description": "Easy GraphQL client integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxtjs",
    "nuxt.js",
    "graphql",
    "graphql-request",
    "nuxt-graphql",
    "nuxt-graphql-request"
  ],
  "typings": "types/index.d.ts",
  "main": "lib/module.js",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "repository": "https://github.com/Gomah/nuxt-graphql-request",
  "homepage": "https://github.com/Gomah/nuxt-graphql-request",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test:system": "NODE_ENV=test jest --testRegex \"/test/system/(.+)\\.test\\.ts$\"",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "watch": "tsc --watch",
    "test": "yarn test:lint && NODE_ENV=test jest",
    "test:lint": "eslint --ext .js, lib/module.js"
  },
  "dependencies": {
    "graphql": "^14.6.0",
    "graphql-request": "^1.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nuxt/types": "^0.6.1",
    "@types/jest": "^25.1.2",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^13.7.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "graphql-tag": "^2.10.3",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "nuxt": "^2.11.0",
    "prettier": "^1.19.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "standard-version": "^7.1.0"
  }
}
